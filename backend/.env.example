# Example environment variables for StackNStay backend
# Copy to .env and fill with your values before running locally

# Required for LLMs / embeddings
GROQ_API_KEY=your_groq_api_key_here
COHERE_API_KEY=your_cohere_api_key_here
LLM_MODEL=llama-3.3-70b-versatile

# Optional: choose vector backend. Set to 'pgvector' to force Postgres.
# If DATABASE_URL is present and VECTOR_BACKEND is 'auto' or 'pgvector', pgvector adapter will be used.
DATABASE_URL=postgresql://user:password@host:5432/dbname
VECTOR_BACKEND=pgvector

# Optional blockchain / IPFS
STACKS_API_URL=
STACKS_CONTRACT_ADDRESS=
IPFS_GATEWAY=
PINATA_JWT=

# Optional observability
SENTRY_DSN=
